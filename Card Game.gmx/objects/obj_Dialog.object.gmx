    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>var bw = sprite_get_width(dialog_sprite) * dialog_xscale;
var bh = sprite_get_height(dialog_sprite) * dialog_yscale;
var th = sprite_get_height(dialog_titlebar) * dialog_yscale;
var dw = dialog_width * dialog_xscale;
var dh = dialog_height * dialog_yscale;

if (dialog_moving) {
    var xdiff = x;
    var ydiff = y;
    
    x = mouse_x - dialog_pickx;
    y = mouse_y - dialog_picky;
    
    xdiff = x - xdiff;
    ydiff = y - ydiff;
    
    closeButton.x += xdiff;
    closeButton.y += ydiff;
}

//bg
draw_rectangle_colour(x+bw, y+bh+th, x+dw-bw, y+dh-bh, dialog_bgcolor, dialog_bgcolor, dialog_bgcolor, dialog_bgcolor, false);

//corners
draw_sprite_ext(dialog_sprite, 0, x, y, dialog_xscale, dialog_yscale, 0, c_white, dialog_alpha);
draw_sprite_ext(dialog_sprite, 1, x+dw-bw, y, dialog_xscale, dialog_yscale, 0, c_white, dialog_alpha);
draw_sprite_ext(dialog_sprite, 2, x+dw-bw, y+dh-bh, dialog_xscale, dialog_yscale, 0, c_white, dialog_alpha);
draw_sprite_ext(dialog_sprite, 3, x, y+dh-bh, dialog_xscale, dialog_yscale, 0, c_white, dialog_alpha);

//bars
draw_sprite_stretched(dialog_sprite, 4, x+bw, y, dw-(bw*2), bh);
draw_sprite_stretched(dialog_sprite, 5, x+dw-bw, y+bh, bw, dh-(bh*2));
draw_sprite_stretched(dialog_sprite, 6, x+bw, y+dh-bh, dw-(bw*2), bh);
draw_sprite_stretched(dialog_sprite, 7, x, y+bh, bw, dh-(bh*2));

//title bar
draw_sprite_stretched(dialog_titlebar, -1, x+bw, y+bh, dw-(bw*2), th);
draw_set_font(fntDialogTitle);
draw_set_halign(fa_left);
draw_set_valign(fa_center);
draw_text_colour(x+bw+2, y+bh+(th/2), dialog_title, c_white, c_white, c_white, c_white, 1);

//cursor
if (mouse_y &lt;= (y + + bh + th)
    &amp;&amp; mouse_y &gt;= y 
    &amp;&amp; mouse_x &gt;= x
    &amp;&amp; mouse_x &lt;= x + dialog_width) {
    if (window_get_cursor() != cr_size_all) {
        window_set_cursor(cr_size_all);
    }
    
    if (!dialog_moving) {
        if (mouse_check_button(mb_left)) {
            dialog_moving=true;
            dialog_pickx = mouse_x - x;
            dialog_picky = mouse_y - y;
        }
    }
} else {
    window_set_cursor(cr_default);
}

if (!mouse_check_button(mb_left)) {
    dialog_moving = false;
}

</string>
          </argument>
