dialog_height=600;
dialog_xscale=1;
dialog_yscale=1;
dialog_alpha=0.75;
dialog_bgcolor=make_colour_rgb(75,75,75);
dialog_sprite=sprite_dialogWindow;
dialog_titlebar=sprite_dialogTitle;
dialog_title="Dialog Box";
dialog_moving = false;
dialog_pickx = 0;
dialog_picky = 0;
x = (window_get_width()/2)-(dialog_width/2);
y = (window_get_height()/2)-(dialog_height/2);

closeButton = instance_create(x+dialog_width-50, y+4, obj_dialogCloseButton);
closeButton.button_owner = id;

</string>
