   {
    if uiBackColor2==0 uiBackColor2=uiBackColor;
    draw_rectangle_color(x, y+ymargin, x+sprite_width-2, y+ymargin+uiLineWidth, uiBackColor,uiBackColor, uiBackColor2,uiBackColor2, uiDrawOutline);   //fill
