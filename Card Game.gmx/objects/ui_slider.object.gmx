var ymargin,disptx,px,slideper,upst;

event_inherited();

if (uiZoomFactor==0.1) || (uiVisible==false) exit;  //for zooming     


disptx=uiTextValue;

draw_set_font(uiFont);
draw_set_halign(uiAlign);
draw_set_valign(fa_top);


